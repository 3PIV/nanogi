{
  "version": "0.1.0",
  "platform": {
    "os": "linux",
    "arch": "amd64"
  },
  "process": {
    "terminal": false,
    "user": {
      "uid": 0,
      "gid": 0,
      "additionalGids": null
    },
    "args": [
      "/nanogi/build/bin/nanogi",
      "-i",
      "scene.yml",
      "-r",
      "bdpt"
    ],
    "env": [
      "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
      "LD_LIBRARY_PATH=/usr/local/lib",
      "TERM=xterm"
    ],
    "cwd": "/data"
  },
  "root": {
    "path": "nanogi-fs",
    "readonly": false
  },
  "hostname": "shell",
  "mounts": [
    {
      "name": "proc",
      "path": "/proc"
    },
    {
      "name": "dev",
      "path": "/dev"
    },
    {
      "name": "devpts",
      "path": "/dev/pts"
    },
    {
      "name": "shm",
      "path": "/dev/shm"
    },
    {
      "name": "mqueue",
      "path": "/dev/mqueue"
    },
    {
      "name": "sysfs",
      "path": "/sys"
    },
    {
      "name": "cgroup",
      "path": "/sys/fs/cgroup"
    },
    {
      "name": "data",
      "path": "/data"
    }
  ],
  "linux": {
    "capabilities": [
      "CAP_AUDIT_WRITE",
      "CAP_KILL",
      "CAP_NET_BIND_SERVICE"
    ]
  }
}
